@{
    ViewBag.Title = "Home Page";
    Layout = "";
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <title>Pharmacy Dashboard</title>
    @if (ViewBag.ServerType != null)
    {
    <script type="text/javascript">
        window.serverType = '@ViewBag.ServerType';
    </script>
    }
    <script type="text/javascript" src="../../Scripts/jquery-1.5.1.min.js"></script>
    <script type="text/javascript" src="../../Scripts/cambian-pharmacy.js"></script>
    <link type="text/css" href="/Content/Pharm.css" rel="Stylesheet" />
    <style type="text/css">
        
        ul.button-list.patient-meds-list.prn-meds li { background-color: Green }
        ul.button-list.patient-meds-list li.complete { color: White; }
        ul.button-list.patient-meds-list li.complete.takenAsDirected { background: #999 url('/Content/tick40.png') no-repeat; background-position: right; }
        ul.button-list.patient-meds-list li.complete.notTakenAsDirected { background: #999 url('/Content/cross40.png') no-repeat; background-position: right; }
        ul.button-list a { text-decoration: none; }
        
        .please-wait.overlay { position: absolute; height: auto; top: 0; bottom: 0; left: 0; right: 0; background-color: Black; opacity: 0.5 }
        .please-wait.spinner { background-color: White; position: absolute; top: 50%; left: 50%; margin-top: -75px; margin-left: -75px; padding: 20px; border: 2px solid black; }
        .please-wait { display: none }
    </style>
</head>
<body class="pharm">
    <div id="container" style="display: none"></div>
    <div id="tap-to-enter" class="wrapper" style="background-color: #91278F; text-align: center"><img src="/Content/tap-to-enter.png" /></div>
    <div class="please-wait overlay"></div>
    <div class="please-wait spinner"><img class="wait-spinner" alt="Please Wait" src="/Content/ajax_loader_gray_128.gif" /></div>
    <div class="login-bar">
        <div class="login-panel">
            <a href="#/Home/Login" id="login-button"><div class="login-buttons">Login</div></a>
            <a href="#/Home/Login" id="logout-button"><div class="login-buttons">Logout</div></a>
            <a href="#"><div class="login-buttons home">Home</div></a>
            <div class="login-buttons go-back">Go Back</div>
            St. Augustines<br /><span id="logged-in-as"></span> <span id="auto-logout"></span>
        </div>
    </div></body>
</html>
